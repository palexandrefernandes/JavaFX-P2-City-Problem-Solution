                  <TableColumn fx:id="description2" prefWidth="75.0" text="Descrição" />
                  <TableColumn fx:id="date2" prefWidth="75.0" text="Data" />
               </columns>
            </TableView>
         </children>
      </HBox>
      <AnchorPane>
         <children>
            <Button mnemonicParsing="false" onAction="#onAddIssuesClick" text="Adicionar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </AnchorPane>
   </children>
