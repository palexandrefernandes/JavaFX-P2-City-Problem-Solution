            <TableView fx:id="table1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="id1" prefWidth="75.0" text="#" />
                <TableColumn fx:id="title1" prefWidth="75.0" text="Titulo" />
                  <TableColumn fx:id="description1" prefWidth="75.0" text="Descrição" />
                  <TableColumn fx:id="date1" prefWidth="75.0" text="Data" />
              </columns>
            </TableView>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <Button fx:id="add" mnemonicParsing="false" onAction="#onAddClick" text="&gt;" />
                  <Button fx:id="remove" mnemonicParsing="false" onAction="#onRemoveClick" text="&lt;" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
